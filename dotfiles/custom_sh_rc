# User specific environment and startup programs
export VISUAL="nvim"

export XDG_CONFIG_HOME="$HOME/.config"
export REPOSITORIES="$HOME/Repositories"
export XDG_PICTURES_DIR="$HOME/Pictures"
export PATH="$PATH:$HOME/.cargo/bin:$HOME/.local/bin:$HOME/bin"

alias dev="cd ~/Projects/"
alias downloads="cd ~/Downloads/"
alias goconf="cd ~/.config/"

export PATH="$PATH:~/.sdkman/candidates/kotlin/current/bin:~/.sdkman/candidates/java/current/bin"

export GOPROXY=https://goproxy.io

export CLASSPATH=".:/home/benedict/.m2/repository/org/antlr/antlr4/4.11.1/antlr4-4.11.1-complete.jar:$CLASSPATH "

alias grun="java org.antlr.v4.runtime.misc.TestRig"

alias vim="vim -S ~/.vimrc"

